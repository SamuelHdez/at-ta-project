  <connectionStrings>
    <add name="DatabaseConnection"
    connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=| DataDirectory|\\Database.mdf;Integrated Security=True"
    providerName="System.Data.SqlClient" />
  </connectionStrings>

